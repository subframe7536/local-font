    "local font"
  ],
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.2.9",
    "@types/node": "^20.6.3",
    "bumpp": "^9.2.0",
    "eslint": "^8.49.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
