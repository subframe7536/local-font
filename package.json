  "version": "0.1.1",
  "description": "wrapper of experimental queryLocalFont API",
  "scripts": {
    "build": "tsup",
    "release": "pnpm run build && bumpp --all -p false && npm publish"
  },
  "repository": "",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "files": [
